package Examples;

public class ArraySum {

	public static void main(String[] args) {
	
		int arr[] = {1,2,3,4,5};
		sum(arr);
	}
	
	public static void sum(int arr1[]) {
		
		int add = 0;
		for (int i=0;i<=arr1.length-1;i++) {
			
			add+=arr1[i];
		}
		System.out.println("The sum of array is: " + add);
	}

}

